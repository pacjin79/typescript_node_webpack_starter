{
    "compilerOptions": {
        "module": "commonjs",
        "types": [
            "node"
        ],
        "target": "es5",
        "moduleResolution": "node",
        "baseUrl": "src",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "outDir": "tsbuid"
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}